    <!-- http://www.q8onto.org/healthcareOntology.owl#DoctorConsultationContext -->

    <owl:Thing rdf:about="&healthcareOntology;DoctorConsultationContext">
        <rdf:type rdf:resource="&healthcareOntology;ClinicalContext"/>
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasPrerequisiteCondition"/>
                <owl:allValuesFrom rdf:resource="&healthcareOntology;Doctor"/>
            </owl:Restriction>
        </rdf:type>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasPrerequisiteCondition"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&healthcareOntology;Appointment"/>
                            <rdf:Description rdf:about="&healthcareOntology;WaitingList"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdf:type>
        <hasSPARQLencoding rdf:datatype="&xsd;string">sparql query string goes here</hasSPARQLencoding>
    </owl:Thing>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#DoctorConsultationPolicy -->

    <owl:Thing rdf:about="&healthcareOntology;DoctorConsultationPolicy">
        <rdf:type rdf:resource="&healthcareOntology;AccessPolicy"/>
        <rdf:type rdf:resource="&owl;NamedIndividual"/>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasValidRecord"/>
                <owl:someValuesFrom rdf:resource="&healthcareOntology;PatientRecord"/>
            </owl:Restriction>
        </rdf:type>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasValidUser"/>
                <owl:someValuesFrom rdf:resource="&healthcareOntology;Doctor"/>
            </owl:Restriction>
        </rdf:type>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasValidRecord"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="&healthcareOntology;Appointment"/>
                            <rdf:Description rdf:about="&healthcareOntology;WaitingList"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdf:type>
        <hasApplicableChain rdf:datatype="&xsd;string">ChainForDoctorLevelAccessToFullMedicalRecord</hasApplicableChain>
        <hasApplicableChain rdf:datatype="&xsd;string">ChainForReadAccessToAppointments</hasApplicableChain>
        <hasApplicableChain rdf:datatype="&xsd;string">ChainForReadAccessToWaitingLists</hasApplicableChain>
        <rdfs:comment rdf:datatype="&xsd;string">To grant doctors full access to the records of patients they are treating during appointments or when they are assigned to the patient on a waiting list.</rdfs:comment>
    </owl:Thing>
    


