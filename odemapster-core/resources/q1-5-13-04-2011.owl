     -->

    


    <!-- http://purl.org/goodrelations/v1#BusinessEntity -->

    <owl:Class rdf:about="&v1;BusinessEntity"/>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#AccessContext -->

    <owl:Class rdf:about="&healthcareOntology;AccessContext">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasValidUserType"/>
                <owl:someValuesFrom rdf:resource="&healthcareOntology;AgentRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasPrerequisiteCondition"/>
                <owl:someValuesFrom rdf:resource="&healthcareOntology;Record"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasApplicablePolicy"/>
                <owl:allValuesFrom rdf:resource="&healthcareOntology;AccessPolicy"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#AccessPolicy -->

    <owl:Class rdf:about="&healthcareOntology;AccessPolicy">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasValidUser"/>
                <owl:someValuesFrom rdf:resource="&healthcareOntology;AgentRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasApplicableChain"/>
                <owl:allValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasValidRecord"/>
                <owl:someValuesFrom rdf:resource="&healthcareOntology;Record"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasSPARQLencoding"/>
                <owl:allValuesFrom rdf:resource="&xsd;string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#AdministrativeContext -->

    <owl:Class rdf:about="&healthcareOntology;AdministrativeContext">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;AccessContext"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#AdministrativeList -->

    <owl:Class rdf:about="&healthcareOntology;AdministrativeList">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;AdministrativeRecord"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#AdministrativeRecord -->

    <owl:Class rdf:about="&healthcareOntology;AdministrativeRecord">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;Record"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#Administrator -->

    <owl:Class rdf:about="&healthcareOntology;Administrator">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;MedicalRelatedStaff"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#Agent -->

    <owl:Class rdf:about="&healthcareOntology;Agent">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;playsRole"/>
                <owl:allValuesFrom rdf:resource="&healthcareOntology;AgentRole"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#AgentRole -->

    <owl:Class rdf:about="&healthcareOntology;AgentRole">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasAccessContext"/>
                <owl:allValuesFrom rdf:resource="&healthcareOntology;AccessContext"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#Appointment -->

    <owl:Class rdf:about="&healthcareOntology;Appointment">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;ScheduledRecord"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasMedicalStaff"/>
                <owl:onClass rdf:resource="&healthcareOntology;MedicalStaff"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#ClinicalContext -->

    <owl:Class rdf:about="&healthcareOntology;ClinicalContext">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;AccessContext"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#ClinicianList -->

    <owl:Class rdf:about="&healthcareOntology;ClinicianList">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;AdministrativeList"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasMedicalStaff"/>
                <owl:allValuesFrom rdf:resource="&healthcareOntology;MedicalStaff"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#ComputationalSystem -->

    <owl:Class rdf:about="&healthcareOntology;ComputationalSystem">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;Agent"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#Doctor -->

    <owl:Class rdf:about="&healthcareOntology;Doctor">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;MedicalStaff"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#DoctorsInstruction -->

    <owl:Class rdf:about="&healthcareOntology;DoctorsInstruction">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;MedicalRecord"/>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#DoctorsLetter -->

    <owl:Class rdf:about="&healthcareOntology;DoctorsLetter">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;MedicalRecord"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasMedicalStaff"/>
                <owl:onClass rdf:resource="&healthcareOntology;Doctor"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#FinancialContext -->

    <owl:Class rdf:about="&healthcareOntology;FinancialContext">
        <rdfs:subClassOf rdf:resource="&healthcareOntology;AccessContext"/>
    </owl:Class>
    


