            <owl:Restriction>
                <owl:onProperty rdf:resource="&healthcareOntology;hasPatient"/>
                <owl:onClass rdf:resource="&healthcareOntology;Patient"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


