        <hasApplicableChain rdf:datatype="&xsd;string">ChainForDoctorLevelAccessToPatientRecord</hasApplicableChain>
        <hasApplicableChain rdf:datatype="&xsd;string">ChainForFullAccessToReferrals</hasApplicableChain>
