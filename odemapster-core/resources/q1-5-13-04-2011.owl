    <owl:DatatypeProperty rdf:about="&healthcareOntology;hasName">
        <rdfs:label rdf:datatype="&xsd;string">hasName</rdfs:label>
        <rdfs:domain rdf:resource="&healthcareOntology;PatientDemographicRecord"/>
        <rdfs:domain rdf:resource="&healthcareOntology;SystemUserRecord"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.q8onto.org/healthcareOntology.owl#hasNursingID -->

    <owl:DatatypeProperty rdf:about="&healthcareOntology;hasNursingID">
