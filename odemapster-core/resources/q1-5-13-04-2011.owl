        <rdfs:domain rdf:resource="&healthcareOntology;ItemsForBilling"/>
        <rdfs:domain rdf:resource="&healthcareOntology;PatientList"/>
        <rdfs:domain rdf:resource="&healthcareOntology;PatientRecord"/>
